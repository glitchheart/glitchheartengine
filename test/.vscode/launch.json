{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "C++ Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/build/main",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"logging": {
				"moduleLoad": false,
				"trace": true,
				"programOutput": true
			}
		},
		{
			"name": "C++ Attach",
			"type": "cppdbg",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	]
}